from django.contrib import admin
from django.contrib.auth import get_user_model

User = get_user_model()


@admin.register(User)  # ã“ã®è¡Œã§ User ãƒ¢ãƒ‡ãƒ«ã‚’ç®¡ç†ç”»é¢ã«ç™»éŒ²
class CustomUserAdmin(admin.ModelAdmin):
    """ãƒ¦ãƒ¼ã‚¶ãƒ¼ç®¡ç†ç”»é¢ã®ã‚«ã‚¹ã‚¿ãƒã‚¤ã‚º"""
    
    # ==================== ä¸€è¦§ç”»é¢ã®è¨­å®š ====================
    
    # ä¸€è¦§ç”»é¢ã«è¡¨ç¤ºã™ã‚‹é …ç›®
    list_display = [
        'employee_id',   # ç¤¾å“¡ç•ªå·
        'username',      # ãƒ¦ãƒ¼ã‚¶ãƒ¼å
        'email',         # ãƒ¡ãƒ¼ãƒ«ã‚¢ãƒ‰ãƒ¬ã‚¹
        'is_admin',      # ç®¡ç†è€…ãƒ•ãƒ©ã‚°
        'is_active',     # ã‚¢ã‚¯ãƒ†ã‚£ãƒ–ãƒ•ãƒ©ã‚°
        'is_deleted',    # å‰Šé™¤æ¸ˆã¿ãƒ•ãƒ©ã‚°ï¼ˆã‚«ã‚¹ã‚¿ãƒ ãƒ¡ã‚½ãƒƒãƒ‰ï¼‰
        'created_at'     # ä½œæˆæ—¥æ™‚
    ]
    
    # çµã‚Šè¾¼ã¿ï¼ˆãƒ•ã‚£ãƒ«ã‚¿ãƒ¼ï¼‰æ©Ÿèƒ½
    # å³å´ã«è¡¨ç¤ºã•ã‚Œã‚‹ãƒ•ã‚£ãƒ«ã‚¿ãƒ¼ãƒ‘ãƒãƒ«
    list_filter = [
        'is_admin',      # ç®¡ç†è€…ã§çµã‚Šè¾¼ã¿
        'is_active',     # ã‚¢ã‚¯ãƒ†ã‚£ãƒ–ã§çµã‚Šè¾¼ã¿
        'is_staff',      # ã‚¹ã‚¿ãƒƒãƒ•ã§çµã‚Šè¾¼ã¿
        'created_at',    # ä½œæˆæ—¥ã§çµã‚Šè¾¼ã¿
        'deleted_at',    # å‰Šé™¤æ—¥ã§çµã‚Šè¾¼ã¿
    ]
    
    # æ¤œç´¢æ©Ÿèƒ½ï¼ˆä¸Šéƒ¨ã®æ¤œç´¢ãƒœãƒƒã‚¯ã‚¹ï¼‰
    search_fields = ['employee_id', 'username', 'email']
    
    # ãƒ‡ãƒ•ã‚©ãƒ«ãƒˆã®ä¸¦ã³é †ï¼ˆæ–°ã—ã„é †ï¼‰
    ordering = ['-created_at']
    
    # ==================== è©³ç´°ç”»é¢ã®è¨­å®š ====================
    
    # è©³ç´°ç”»é¢ã®ãƒ•ã‚£ãƒ¼ãƒ«ãƒ‰ã‚’ã‚°ãƒ«ãƒ¼ãƒ—åˆ†ã‘
    fieldsets = (
        # ã‚°ãƒ«ãƒ¼ãƒ—1: èªè¨¼æƒ…å ±
        ('èªè¨¼æƒ…å ±', {
            'fields': ('employee_id', 'password')
        }),
        # ã‚°ãƒ«ãƒ¼ãƒ—2: å€‹äººæƒ…å ±
        ('å€‹äººæƒ…å ±', {
            'fields': ('username', 'email')
        }),
        # ã‚°ãƒ«ãƒ¼ãƒ—3: æ¨©é™è¨­å®š
        ('æ¨©é™', {
            'fields': (
                'is_active',        # ã‚¢ã‚¯ãƒ†ã‚£ãƒ–
                'is_admin',         # ç®¡ç†è€…
                'is_staff',         # ã‚¹ã‚¿ãƒƒãƒ•ï¼ˆç®¡ç†ç”»é¢ãƒ­ã‚°ã‚¤ãƒ³OKï¼‰
                'is_superuser',     # ã‚¹ãƒ¼ãƒ‘ãƒ¼ãƒ¦ãƒ¼ã‚¶ãƒ¼ï¼ˆå…¨æ¨©é™ï¼‰
                'groups',           # ã‚°ãƒ«ãƒ¼ãƒ—
                'user_permissions', # å€‹åˆ¥æ¨©é™
            )
        }),
        # ã‚°ãƒ«ãƒ¼ãƒ—4: ã‚¿ã‚¤ãƒ ã‚¹ã‚¿ãƒ³ãƒ—ï¼ˆæŠ˜ã‚ŠãŸãŸã¿è¡¨ç¤ºï¼‰
        ('ã‚¿ã‚¤ãƒ ã‚¹ã‚¿ãƒ³ãƒ—', {
            'fields': ('created_at', 'updated_at', 'deleted_at', 'last_login'),
            'classes': ('collapse',),  # æœ€åˆã¯æŠ˜ã‚ŠãŸãŸã¾ã‚Œã¦ã„ã‚‹
        }),
    )
    
    # æ–°è¦ä½œæˆæ™‚ã®ãƒ•ã‚£ãƒ¼ãƒ«ãƒ‰ï¼ˆãƒ‘ã‚¹ãƒ¯ãƒ¼ãƒ‰ç¢ºèªãŒå¿…è¦ï¼‰
    add_fieldsets = (
        ('èªè¨¼æƒ…å ±ï¼ˆå¿…é ˆï¼‰', {
            'classes': ('wide',),
            'fields': ('employee_id', 'password1', 'password2'),
        }),
        ('å€‹äººæƒ…å ±ï¼ˆä»»æ„ï¼‰', {
            'fields': ('username', 'email'),
        }),
        ('æ¨©é™', {
            'fields': ('is_active', 'is_admin', 'is_staff', 'is_superuser'),
        }),
    )
    
    # èª­ã¿å–ã‚Šå°‚ç”¨ãƒ•ã‚£ãƒ¼ãƒ«ãƒ‰ï¼ˆç·¨é›†ä¸å¯ï¼‰
    readonly_fields = ['created_at', 'updated_at', 'deleted_at', 'last_login']
    
    # ==================== ã‚«ã‚¹ã‚¿ãƒ ãƒ¡ã‚½ãƒƒãƒ‰ ====================
    
    @admin.display(boolean=True, description='å‰Šé™¤æ¸ˆã¿')
    def is_deleted(self, obj):
        """
        å‰Šé™¤æ¸ˆã¿ã‹ã©ã†ã‹ã‚’è¡¨ç¤ºã™ã‚‹ã‚«ã‚¹ã‚¿ãƒ é …ç›®
        
        å¼•æ•°:
            obj: ãƒ¦ãƒ¼ã‚¶ãƒ¼ã‚ªãƒ–ã‚¸ã‚§ã‚¯ãƒˆ
        
        æˆ»ã‚Šå€¤:
            True: âœ“ ãƒãƒ¼ã‚¯ãŒè¡¨ç¤ºã•ã‚Œã‚‹
            False: âœ— ãƒãƒ¼ã‚¯ãŒè¡¨ç¤ºã•ã‚Œã‚‹
        """
        return obj.deleted_at is not None
    
    # ==================== ä¸€æ‹¬ã‚¢ã‚¯ã‚·ãƒ§ãƒ³ ====================
    
    # ä½¿ç”¨å¯èƒ½ãªã‚¢ã‚¯ã‚·ãƒ§ãƒ³ï¼ˆä¸€è¦§ç”»é¢ã®ä¸Šéƒ¨ã«è¡¨ç¤ºï¼‰
    actions = ['restore_users', 'soft_delete_users']
    
    @admin.action(description='é¸æŠã—ãŸãƒ¦ãƒ¼ã‚¶ãƒ¼ã‚’å¾©å…ƒ')
    def restore_users(self, request, queryset):
        """
        é¸æŠã—ãŸãƒ¦ãƒ¼ã‚¶ãƒ¼ã‚’ä¸€æ‹¬å¾©å…ƒ
        
        ä½¿ã„æ–¹:
        1. ä¸€è¦§ç”»é¢ã§å¾©å…ƒã—ãŸã„ãƒ¦ãƒ¼ã‚¶ãƒ¼ã«ãƒã‚§ãƒƒã‚¯
        2. ã‚¢ã‚¯ã‚·ãƒ§ãƒ³é¸æŠã§ã€Œé¸æŠã—ãŸãƒ¦ãƒ¼ã‚¶ãƒ¼ã‚’å¾©å…ƒã€ã‚’é¸ã¶
        3. å®Ÿè¡Œ
        """
        count = 0
        for user in queryset:
            # å‰Šé™¤æ¸ˆã¿ãƒ¦ãƒ¼ã‚¶ãƒ¼ã®ã¿å¾©å…ƒ
            if user.deleted_at:
                user.restore()  # models.py ã® restore ãƒ¡ã‚½ãƒƒãƒ‰
                count += 1
        
        # å®Œäº†ãƒ¡ãƒƒã‚»ãƒ¼ã‚¸ã‚’è¡¨ç¤º
        self.message_user(request, f'{count}äººã®ãƒ¦ãƒ¼ã‚¶ãƒ¼ã‚’å¾©å…ƒã—ã¾ã—ãŸã€‚')
    
    @admin.action(description='é¸æŠã—ãŸãƒ¦ãƒ¼ã‚¶ãƒ¼ã‚’è«–ç†å‰Šé™¤')
    def soft_delete_users(self, request, queryset):
        """
        é¸æŠã—ãŸãƒ¦ãƒ¼ã‚¶ãƒ¼ã‚’ä¸€æ‹¬è«–ç†å‰Šé™¤
        
        ä½¿ã„æ–¹:
        1. ä¸€è¦§ç”»é¢ã§å‰Šé™¤ã—ãŸã„ãƒ¦ãƒ¼ã‚¶ãƒ¼ã«ãƒã‚§ãƒƒã‚¯
        2. ã‚¢ã‚¯ã‚·ãƒ§ãƒ³é¸æŠã§ã€Œé¸æŠã—ãŸãƒ¦ãƒ¼ã‚¶ãƒ¼ã‚’è«–ç†å‰Šé™¤ã€ã‚’é¸ã¶
        3. å®Ÿè¡Œ
        """
        count = 0
        for user in queryset:
            # æœªå‰Šé™¤ã®ãƒ¦ãƒ¼ã‚¶ãƒ¼ã®ã¿å‰Šé™¤
            if not user.deleted_at:
                user.soft_delete()  # models.py ã® soft_delete ãƒ¡ã‚½ãƒƒãƒ‰
                count += 1
        
        # å®Œäº†ãƒ¡ãƒƒã‚»ãƒ¼ã‚¸ã‚’è¡¨ç¤º
        self.message_user(request, f'{count}äººã®ãƒ¦ãƒ¼ã‚¶ãƒ¼ã‚’è«–ç†å‰Šé™¤ã—ã¾ã—ãŸã€‚')
    
    # ==================== è¿½åŠ è¨­å®š ====================
    
    def get_queryset(self, request):
        """
        ä¸€è¦§ã«è¡¨ç¤ºã™ã‚‹ãƒ¦ãƒ¼ã‚¶ãƒ¼ã‚’å–å¾—
        
        é€šå¸¸ã¯å‰Šé™¤æ¸ˆã¿ãƒ¦ãƒ¼ã‚¶ãƒ¼ã¯è¡¨ç¤ºã•ã‚Œãªã„ãŒã€
        ç®¡ç†ç”»é¢ã§ã¯å‰Šé™¤æ¸ˆã¿ã‚‚å«ã‚ã¦ã™ã¹ã¦è¡¨ç¤ºã™ã‚‹
        """
        return User.all_objects.all()  # å‰Šé™¤æ¸ˆã¿ã‚‚å«ã‚€
    
    def get_form(self, request, obj=None, **kwargs):
        """
        ç·¨é›†ãƒ•ã‚©ãƒ¼ãƒ ã‚’ã‚«ã‚¹ã‚¿ãƒã‚¤ã‚º
        
        æ–°è¦ä½œæˆã¨ç·¨é›†ã§ç•°ãªã‚‹ãƒ•ã‚£ãƒ¼ãƒ«ãƒ‰ã‚»ãƒƒãƒˆã‚’ä½¿ã†
        
        å¼•æ•°:
            obj: ç·¨é›†å¯¾è±¡ã®ãƒ¦ãƒ¼ã‚¶ãƒ¼ï¼ˆNone ãªã‚‰æ–°è¦ä½œæˆï¼‰
        """
        if obj is None:
            # æ–°è¦ä½œæˆæ™‚: password1, password2 ã‚’ä½¿ã†
            self.fieldsets = self.add_fieldsets
        else:
            # ç·¨é›†æ™‚: é€šå¸¸ã®ãƒ•ã‚£ãƒ¼ãƒ«ãƒ‰ã‚»ãƒƒãƒˆ
            self.fieldsets = (
                ('èªè¨¼æƒ…å ±', {'fields': ('employee_id', 'password')}),
                ('å€‹äººæƒ…å ±', {'fields': ('username', 'email')}),
                ('æ¨©é™', {
                    'fields': (
                        'is_active', 'is_admin', 'is_staff', 'is_superuser',
                        'groups', 'user_permissions',
                    )
                }),
                ('ã‚¿ã‚¤ãƒ ã‚¹ã‚¿ãƒ³ãƒ—', {
                    'fields': ('created_at', 'updated_at', 'deleted_at', 'last_login'),
                    'classes': ('collapse',),
                }),
            )
        return super().get_form(request, obj, **kwargs)
    
    def save_model(self, request, obj, form, change):
        """
        ãƒ¦ãƒ¼ã‚¶ãƒ¼ã‚’ä¿å­˜ã™ã‚‹æ™‚ã®å‡¦ç†
        
        ãƒ‘ã‚¹ãƒ¯ãƒ¼ãƒ‰ã‚’ãƒãƒƒã‚·ãƒ¥åŒ–ã—ã¦ä¿å­˜
        
        å¼•æ•°:
            request: ãƒªã‚¯ã‚¨ã‚¹ãƒˆæƒ…å ±
            obj: ä¿å­˜ã™ã‚‹ãƒ¦ãƒ¼ã‚¶ãƒ¼ã‚ªãƒ–ã‚¸ã‚§ã‚¯ãƒˆ
            form: ãƒ•ã‚©ãƒ¼ãƒ ãƒ‡ãƒ¼ã‚¿
            change: True=ç·¨é›†, False=æ–°è¦ä½œæˆ
        """
        if not change:
            # æ–°è¦ä½œæˆæ™‚: password1 ã‚’ãƒãƒƒã‚·ãƒ¥åŒ–
            obj.set_password(form.cleaned_data['password1'])
        elif 'password' in form.changed_data:
            # ãƒ‘ã‚¹ãƒ¯ãƒ¼ãƒ‰å¤‰æ›´æ™‚: ãƒãƒƒã‚·ãƒ¥åŒ–
            obj.set_password(form.cleaned_data['password'])
        
        # ä¿å­˜
        super().save_model(request, obj, form, change)


# ==================== ä½¿ã„æ–¹ã¾ã¨ã‚ ====================
"""
Djangoç®¡ç†ç”»é¢ã®ä½¿ã„æ–¹:

1. ã‚¹ãƒ¼ãƒ‘ãƒ¼ãƒ¦ãƒ¼ã‚¶ãƒ¼ã§ãƒ­ã‚°ã‚¤ãƒ³
   python manage.py createsuperuser

2. ç®¡ç†ç”»é¢ã«ã‚¢ã‚¯ã‚»ã‚¹
   http://localhost:8000/admin/

3. ã§ãã‚‹ã“ã¨:
   - ãƒ¦ãƒ¼ã‚¶ãƒ¼ä¸€è¦§ã®è¡¨ç¤ºï¼ˆå‰Šé™¤æ¸ˆã¿ã‚‚å«ã‚€ï¼‰
   - ãƒ¦ãƒ¼ã‚¶ãƒ¼ã®æ¤œç´¢ãƒ»çµã‚Šè¾¼ã¿
   - ãƒ¦ãƒ¼ã‚¶ãƒ¼ã®ä½œæˆãƒ»ç·¨é›†ãƒ»å‰Šé™¤
   - å‰Šé™¤æ¸ˆã¿ãƒ¦ãƒ¼ã‚¶ãƒ¼ã®å¾©å…ƒ
   - ä¸€æ‹¬æ“ä½œï¼ˆè¤‡æ•°é¸æŠã—ã¦å‰Šé™¤ãƒ»å¾©å…ƒï¼‰
"""

********************************************************************************

